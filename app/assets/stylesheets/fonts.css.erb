@font-face {
  font-family: 'nta';
  src: url(<%= asset_path "nta_light-webfont-ps.eot" %>#) format('embedded-opentype'),
       url(<%= asset_path "nta_light-webfont-ps.woff" %>) format('woff');
  font-weight: normal;
  font-style: normal;
}

@font-face {
  font-family: 'nta-bold';
  src: url(<%= asset_path "nta_medium-webfont-ps.eot" %>#) format('embedded-opentype'),
       url(<%= asset_path "nta_medium-webfont-ps.woff" %>) format('woff');
  font-weight: normal;
  font-style: normal;
}
