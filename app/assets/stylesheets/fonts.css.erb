@font-face {
  font-family: 'nta';
  src: url(<%= asset_path "nta_light-webfont.eot#" %>) format('embedded-opentype'),
       url(<%= asset_path "nta_light-webfont.woff" %>) format('woff');
  font-weight: normal;
  font-style: normal;
}

@font-face {
  font-family: 'nta-bold';
  src: url(<%= asset_path "nta_medium-webfont.eot#" %>) format('embedded-opentype'),
       url(<%= asset_path "nta_medium-webfont.woff" %>) format('woff');
  font-weight: normal;
  font-style: normal;
}
