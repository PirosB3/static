}

header.page-header hgroup h1 {
/*  background-position: 0 2px;*/
  background-repeat: no-repeat;
  color: #000;
  font-family: 'Gill Sans W01 Medium', sans-serif;
  font-size: 2.75em;
  font-weight: normal;
  letter-spacing: 0.025em;
  line-height: 1.05em;
  padding: 0.1em 1.5em 0 1.65em;
}
.lte-ie8 header.page-header hgroup h1 {
  height: 1%;
}

.service header.page-header hgroup h1 {
  background-position: 0 9px;
}
.guide header.page-header hgroup h1 {
  background-position: 15px 2px;
}
.answer header.page-header hgroup h1 {
  background-position: 12px 4px;
}
.programme header.page-header hgroup h1 {
