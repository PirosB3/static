/*
 * auto complete
 * borrowed from the alpha
 */
.hint-suggest{
  border: 1px solid #EEEEEE;
  margin:0;
  background-color:#fff;
  display: block;
  left: 0;
  padding: 0.5em;
  position: relative;
  top: 0;
  width: 163px;
  color:#666;
  font-size:0.95em;
}

.search-loading{
  background: url(<%= asset_path "ajax-loader.gif" %>) no-repeat 4px 50% #fff;
  padding-left:30px;
  width: 133px;
}
#main_autocomplete {
  color: inherit;
}

ul.ui-menu {
  padding: 0;
}
ul.ui-widget {
  font-size: 1em;
}
ul.ui-autocomplete {
  background: none repeat scroll 0 0 #fff;
  border: 1px solid #8F8F8F;
  list-style-type:none;
  padding:0;
  margin:0;
  margin-top:1px;
  position:absolute;
  box-shadow: 0 2px 3px rgba(0, 0, 0, 0.25);
}

/* remember that these are used across the site, not just for autocomplete */
li.guide a, li.guide a:hover {
  background-image: url(<%= asset_path "icon-guide-s.png" %>);
  background-repeat: no-repeat;
}
li.answer a, li.answer a:hover,
li.calendar a, li.calendar a:hover,
li.smart_answer a, li.smart_answer a:hover,
li.calculator a, li.calculator a:hover,
li.custom-application a, li.custom-application a:hover {
