  background-repeat: no-repeat;
}
li.local_transaction a, li.local_transaction a:hover,
li.transaction a, li.transaction a:hover,
li.place a, li.place a:hover,
li.nearest a, li.nearest a:hover,
li.service a, li.service a:hover {
  background-image: url(<%= asset_path "icon-service-s.png" %>);
  background-repeat: no-repeat;
}
li.programme a, li.programme a:hover {
  background-image: url(<%= asset_path "icon-programme-s.png" %>);
  background-repeat: no-repeat;
}

ul.ui-autocomplete li {
  font-size: 0.75em;
}
ul.ui-autocomplete li {
  border-top: 1px solid #eee;
  list-style-type:none;
  margin:0;
}
ul.ui-autocomplete.ui-menu .ui-menu-item a,
ul.ui-autocomplete .search-site a {
  padding: 0.2em 1em;
  display:block;
  color: #333;
  text-decoration:none;
}
ul.ui-autocomplete .search-site a,
ul.ui-autocomplete .search-site a:hover {
  border-top:3px solid #dedede;
  background: url(<%= asset_path "search.png" %>) no-repeat 12px 50% #eee;
  padding-left: 3em;
}
ul.ui-autocomplete li a em {
  font-style: normal;
  font-weight: bold;
  padding: 0.125em;
}
ul.ui-menu .ui-menu-item a.ui-state-hover,
ul.ui-menu .ui-menu-item a.ui-state-active,
