html, body, div, h1, h2, h3, h4, h5, h6, article, aside, footer, header, hgroup, nav, section {
  margin: 0;
  padding: 0;
  vertical-align: baseline;
}

/* self-clearing floats */
.group:before, .group:after { content: "\0020"; display: block; height: 0; overflow: hidden; }
.group:after { clear: both; }
/* Fix clearfix */
.group { zoom: 1; }

/*
 * Core global styles
 */

html {
  background: #C9D0D0;
}

body {
  color: #333;
  font-family: "Helvetica Neue", Arial, Helvetica, sans-serif;
  line-height: 1.5em;
}
.visuallyhidden {
  display:none;
}
#wrapper {
  background-color: #F2F2F2;
}

.inner {
  margin: 0 auto;
  padding: 1em;
  position: relative;
}

section#content {
  margin: 0 auto;
  max-width: 960px;
/*  width: 100%;*/
}
.lte-ie8 section#content {
  _width: 960px;
}

/*@media only screen and (min-width:670px) {
  section#content {
    margin: 1em auto 1em;
  }
  .js-enabled section#content {
    margin: 6em auto 2em;
  }
}
.lte-ie8 section#content {
  margin: 1em auto 1em;
}
.lte-ie8 .js-enabled section#content {
  margin: 6em auto 2em;
}*/

ol, ul, nav ol, nav ul {
  list-style: inherit;
}

fieldset {
  border: none;
  padding: 0;
}

h2 {
  font-size: 1.35em;
  line-height: 1.18em;
  margin-top: 0.5em;
  margin-bottom: 0.25em;
}

h3 {
  font-size: 1.2em;
  line-height: 1.11em;
  margin-top: 0.5em;
  margin-bottom: 0.25em;
}

h4 {
  margin-top: 0.5em;
  margin-bottom: 0.25em;
}

/*
 * Global link styles
 */

a,
a:visited {
  color: #2E3191;
}

a:hover {
  color: #2E8ACA;
}

a:active {
  color: #c11;
}

a[rel="external"]:after {
  content: url(<%= asset_path "external-link.png" %>);
  margin-right: 0.3em;
}

/*
 * Locate me styles
 */

.popover-mask {
  background-color: rgba(0,0,0,0.5);
  display: none;
  height: 100%;
  left: 0;
  margin: 0;
  top: 30px;
  padding: 0;
  position: fixed;
  width: 100%;
  z-index: 60;
}

.popover-mask .popover {
  background: #fafafa;
  border: solid 2px #bbb;
  -moz-border-radius: 0.5em;
  -webkit-border-radius: 0.5em;
  border-radius: 0.5em;
  box-shadow: 4px 4px 10px rgba(0,0,0,0.3);
  margin: 4em auto 0 auto;
  padding: 0 2.5em 2em 2.5em;
  position: relative;
  min-width: 25em;
}

.popover .close {
  position: absolute;
  top: -19px;
  right: -19px;
}

.popover h2 {
  margin: 0.5em 0;
  padding-bottom: 0.2em;
}

#global-locator-form label,
#global-locator-form .ask_location{
  display: inline;
}
#global-locator-form .ask_location.hidden { display: none; }


/*
 * Related box
 * this related stuff needs re-examining - might be a bit over the top
 */

.related-positioning {
  height: 0;
  left: 0;
  position: fixed;
  top: 8.15em;
  width: 100%;
}
.lte-ie8 .related-positioning {
  _position: absolute !important;
  z-index: 10;
}

#wrapper.programme .related-positioning {
  /*  top: 12.1em;*/
}

.related-container {
  height: 0;
  margin: 0 auto;
  max-width: 960px;
  position: relative;
  z-index: 50;
}
.lte-ie8 .related-container {
  _width: 960px;
}

.related {
  position: absolute;
  right: 0;
  width: 16.5em;
/*  width: 14em;*/
}

.related .inner {
  background-color: #F0E7D5;
  padding: 0 1em 0.25em 1.5em;
  width: auto;
}
.wf-gillsans-n4-inactive h2{
  font-family: "Helvetica Neue", Arial, Helvetica, sans-serif;
}
.wf-gillsans-n4-loading h2{
  font-family: "Helvetica Neue", Arial, Helvetica, sans-serif;
}
.wf-gillsans-n4-active h2{
  font-family: 'Gill Sans W01 Medium', sans-serif;
}
.related h2 {
  background-image: url(<%= asset_path "related-clip.png" %>);
  background-position: 0 0;
  background-repeat: no-repeat;
  color: #000;
  font-family: 'Gill Sans W01 Medium', sans-serif;
  font-size: 0.85em;
  font-weight: normal;
  height: 30px;
  letter-spacing: 0.15em;
  line-height: 1.5em;
  margin: 0 0 0 -21px;
  padding: 30px 0 0 54px;
  position: relative;
  text-transform: uppercase;
  top: -13px;
}
.lte-ie8 .related {
  display: inline;
  height: 1%;
  zoom: 1;
}

.related ul {
  list-style: none;
  margin: 0;
  padding: 0;
}

.related li {

}

.related li a {
  background-position: 0 -290px;
  background-repeat: no-repeat;
  display: block;
  font-size: 0.85em;
  line-height: 1.5em;
  margin-bottom: 0.75em;
  padding: 0.25em 0 0.5em 2.5em;
}
.related li a:hover {
  background-position: 0 0;
}

.lte-ie8 .related li a {
  height: 1%;
}

.related li a[href*="#"] {
  /* grey out stub links */
  background: none;
  color: #888;
  cursor: default;
  text-decoration: none;
}

.related li.related-topic {
  border-top: solid #B4892D 1px;
  list-style: none;
  margin-top: 0.5em;
  padding-top: 0.5em;
}

.related li.related-topic span {
/*  display: block;*/
  font-weight: bold;
}

.related li.related-topic a {
/*  color: inherit;*/
  padding-left: 0;
}

.related li.related-topic span.arrow {
  display: none;
}

/*
  page headers
*/

header.page-header hgroup {
  background-color: #fff;
  border-style: solid;
  border-width: 2px 0 0 0;
