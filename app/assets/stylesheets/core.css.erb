/* buttons! */
.button,
.button:visited {
  background-color: #B01117;
  border: none;
  color: #fff;
  cursor: pointer;
  display: inline-block;
  padding: 0.5em 0.75em;
  position: relative;
  text-decoration: none;
  -webkit-transition: background-color 0.5s ease-in-out;
  -moz-transition: background-color 0.5s ease-in-out;
}
.button:hover {
  background-color: #DD322D;
  color: #fff;
}
.button:active {
  background-color: #DD322D;
  top: 1px;
}
.button:focus {
  background-color: #001950;
}

input.button {
  padding: 0.25em 0.5em;
}

.button.medium {
  font-size: 1.25em;
}

.button.big {
  font-family: 'Gill Sans W01 Medium', sans-serif;
  font-size: 1.75em;
  letter-spacing: 0.15em;
  padding: 0.5em 0.4em;
  text-transform: uppercase;
}

.get-started {
  margin: 0;
  text-align: center;
}

.get-started .button {
/*  float: right;*/
  font-family: 'Gill Sans W01 Medium', sans-serif;
  margin: 0.75em 0 0 0;
  max-width: 60%;
  position: relative;
/*  padding: 0.325em 1.5em 0.325em 0;*/
/*  padding: 0.75em 0.75em;*/
}
.lte-ie8 .get-started .button {
  _width: 60%;
}

.get-started .button.big {
  background-image: url(<%= asset_path "button-arrow.png" %>);
  background-position: 95% 50%;
  background-repeat: no-repeat;
  padding: 0.65em 2em 0.65em 0.5em;
}

.get-started .button .button-content {
  display: inline-block;
/*  padding-right: 0.75em;*/
  vertical-align: middle;
/*  margin-right: 2em;*/
}

.get-started .button .destination {
  display: block;
  font-family: "Helvetica Neue", Arial, Helvetica, sans-serif;
  letter-spacing: 0;
  margin: 0.25em 0 0 0;
  opacity: 0.75;
  text-transform: none;
}

.wf-gillsans-n4-inactive .button .call-to-action {
  font-family: "Helvetica Neue", Arial, Helvetica, sans-serif;
}
.wf-gillsans-n4-loading .button .call-to-action {
  font-family: "Helvetica Neue", Arial, Helvetica, sans-serif;
}
.wf-gillsans-n4-active .button .call-to-action {
  font-family: 'Gill Sans W01 Medium', sans-serif;
}
.get-started .button .call-to-action {
  background-image: url(<%= asset_path "button-arrow.png" %>);
  background-position: 9em 50%;
  background-repeat: no-repeat;
  display: block;
  font-size: 1.5em;
  letter-spacing: 0.15em;
  margin: 0 auto;
  padding: 0.325em 1.5em 0.325em 0;
  text-transform: uppercase;
  width: 9em;
}

